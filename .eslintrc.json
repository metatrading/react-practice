{
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "node",
    "react"
  ],
  "env": {
    "browser": true,
    "node" : true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },

  "globals": {},
  "rules": {
    "react/prop-types":0,
    "node/no-unsupported-features/es-syntax":0
    //    "react/jsx-uses-react": "error",
//    "react/jsx-uses-vars": "error"
  }
}